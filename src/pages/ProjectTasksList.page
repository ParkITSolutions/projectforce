													</apex:outputPanel>	
											
													<apex:outputPanel rendered="{!IF( o.fieldType != 'PICKLIST' && o.fieldType != 'BOOLEAN'&& o.fieldType != 'DATE', true, false) }" layout="block" styleClass="DymFilElem">
