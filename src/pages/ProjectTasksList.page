														<!-- {!o.name}::{!o.FieldType} -->
													</apex:outputPanel>	

													<apex:outputPanel rendered="{!IF( o.fieldType == 'DATE', true, false) }" layout="block" styleClass="DymFilElem">
														<span class="DynFIlLabel">{!o.label}</span><br />
